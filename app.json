{
  "expo": {
    "name": "mrs-paquetes-mobile",
    "slug": "mrs-paquetes-mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logo-claro.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/logo-claro.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/logo-claro.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.jisaherrera.mrspaquetesmobile"
    },
    "web": {
      "favicon": "./assets/logo-claro.png"
    },
    "plugins": [
      [
        "expo-build-properties",
        {
          "android": {
            "usesCleartextTraffic": true,
            "compileSdkVersion": 34,
            "targetSdkVersion": 34,
            "buildToolsVersion": "34.0.0"
          }
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "f5f8c0ac-9537-424d-b101-028b8e83bff2"
      }
    }
  }
}